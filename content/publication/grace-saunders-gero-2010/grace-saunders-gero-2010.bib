@inproceedings{grace-saunders-gero:2010,
 abstract = {This paper reports on a system for computational analogy-making based on conceptual spaces. The system constructs conceptual spaces that express the relationships between concepts and uses them to build new associations. A case for this conceptual-space driven model of association making is made, and its advantages and disadvantages are discussed. A prototype space-construction system is detailed and one method by which such a system could be used to make associations is proposed. The system forms concepts that are useful to describe a set of objects, then learns how those concepts relate to each other. These relationships can then be used to construct analogies.},
 address = {Lisbon, Portugal},
 author = {Kazjon Grace and Rob Saunders and John S. Gero},
 bdsk-file-1 = {},
 booktitle = {Proceedings of the First International Conference on Computational Creativity},
 date-added = {2019-05-31 16:38:38 +0100},
 date-modified = {2019-05-31 16:38:38 +0100},
 keywords = {analogy making, design computing, computational creativity},
 month = {7-9 January},
 series = {ICCC-X},
 title = {Constructing conceptual spaces for novel associations},
 year = {2010}
}

